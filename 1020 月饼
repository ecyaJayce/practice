#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include<stdlib.h>
typedef struct 
{
	double VAL;
	double STR;
	double AVE;
}yuebing;
int cmp_int(const void* e1, const void* e2)
{
	if (((yuebing*)e1)->AVE > ((yuebing*)e2)->AVE)
		return -1;
	else if (((yuebing*)e1)->AVE == ((yuebing*)e2)->AVE)
		return 0;
	else
		return 1;
}
int main()
{
	int i = 0;
	int N = 0;
	double D = 0;
	double sum = 0;
	yuebing ARR[1000] = {};
	scanf("%d %lf", &N, &D);
	for (i = 0; i < N; i++)
	{
		scanf("%lf", &ARR[i].STR);
	}
	for (i = 0; i < N; i++)
	{
		scanf("%lf", &ARR[i].VAL);
	}
	for (i = 0; i < N; i++)
	{
		ARR[i].AVE = ARR[i].VAL / ARR[i].STR;
	}
	qsort(ARR, N, sizeof(ARR[0]), cmp_int);
	for (i = 0; i < N; i++)
	{
		if (D > 0)
		{
			if (D > ARR[i].STR)
			{
				sum = sum + ARR[i].VAL;
				D = D - ARR[i].STR;
				continue;
			}
			else
			{
				sum = sum + D * ARR[i].AVE;
				D = 0;
				break;
			}
		}
	}
	printf("%.2lf", sum);
	return 0;
}

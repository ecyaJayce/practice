#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int my_pow(int x, int y)
{
	int i = 0;
	int sum = 1;
	for (i = 0; i < y; i++)
	{
		sum = sum * x;
	}
	return sum;
}
int main()
{
	int num;
	int arr[10] = {};
	int input = 0;
	for (num = 1; num <= 100000; num++)
	{
		int i = 0;
		int sum = 0;
		int n = 0;
		input = num;
		while (input != 0)
		{
			arr[n] = input % 10;
			input = input / 10;
			n++;
		}
		for (i = 0; i < n; i++)
		{
			sum = sum + my_pow(arr[i], n);
		}
		if (num == sum)
			printf("%d\n", num);
	}
	return 0;
}

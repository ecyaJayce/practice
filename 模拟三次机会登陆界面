#include<stdio.h>
#include<string.h>

int main(void)
{
	int i=0;
	char password[20]={0};
	for (i=0;i<3;i++)
	{
		printf(“请输入密码：”);
		scanf(“%s,password”);
		if(strcmp(password,“123456”))
		{
			printf(“登录成功！”);
			break;
		}
		else
			printf(“密码错误”);
	}
if(i==3)
	printf(“三次密码均错误，退出程序\n”);
return 0;
}

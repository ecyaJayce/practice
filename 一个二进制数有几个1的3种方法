#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int count_bit_one_1(int a)
{
	int count = 0;
	while (a)
	{
		a=a& (a - 1);
		count++;
	}
	return count;
}
int count_bit_one_2(int a)
{
	int count = 0;
	while (a)
	{
		if (a%2==1)
		{
			count++;
		}
		a = a >> 1;
	}
	return count;
}
int count_bit_one_3(int a)
{
	int count = 0;
	int i = 0;
	for (i = 0; i < 32; i++)
	{
		if (((a >> i) & 1) == 1)
			count++;
	}
	return count;
}
int main()
{
	int a = 0;
	scanf("%d", &a);
	int count1 = count_bit_one_1(a);
	int count2 = count_bit_one_2(a);
	int count3 = count_bit_one_2(a);
	printf("%d %d %d",count1,count2,count3);
	return 0;
}
